URxvt.geometry: 80x30
urxvt*scrollBar: false
!URxvt.borderLess: true
URxvt.buffered: true
URxvt.background: black
URxvt.foreground: white
URxvt.cursorColor: #7AC5CD
URxvt.underlineColor: cyan
URxvt.font:             xft:terminus:pixelsize=14:antialias=false
!URxvt.font: -*-terminus-medium-*-*-*-*-140-*-*-*-*-iso8859-1

!------------
!True Transparency
!------------
!set to 32-bit for real transparency (compositing required (see xcompmgr)
urxvt*depth:             32
!transparent=0000 opaque=ffff
urxvt*background: rgba:1111/1111/1111/cccc
!urxvt*background: rgba:1111/1111/1111/0000


!------------------
!False tranparency
!------------------
!URxvt.transparent: true
!URxvt.shading: 70 

Urxvt.secondaryScroll:  true    # Enable Shift-PageUp/Down in screen

Xft*dpi: 96
Xft*antialias: 1
Xft*hinting: 1
Xft*hintstyle: hintfull
Xft*rgba: rgb

*fading:                40
*fadeColor:             #000000


!! blacks: Black / Dark grey
*color0:                #000000
*color8:                #3D3D3D
!! reds: Strong red / Light brilliant red
*color1:                #B80000
*color9:                #FF3333
!! greens: Light brilliant green / Light brilliant chartreuse green
*color2:                #33FF33
*color10:               #99FF33
!! yellows: Light brilliant orange / Light brilliant yellow
*color3:                #FF9933
*color11:               #FFFF33
!! blues: Light brilliant blue / Light brilliant azure
*color4:                #3333FF
*color12:               #3399FF
!! magentas: Light brilliant magenta / Light brilliant violet
*color5:                #FF33FF
*color13:               #9933FF
!! cyans: Light brilliant cyan / Very light cyan
*color6:                #33FFFF
*color14:               #99FFFF
!! whites: Cosmic latte / White
*color7:                #FFF8E7
*color15:               #FFFFFF

